    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/touv/node-ezs-booster.git"
  },
  "keywords": [
    "ezs"
  ],
  "author": "Nicolas Thouvenin <nthouvenin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/touv/node-ezs-booster/issues"
  },
  "homepage": "https://github.com/touv/node-ezs-booster#readme",
  "dependencies": {
